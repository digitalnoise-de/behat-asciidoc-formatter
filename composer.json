{
    "name": "digitalnoise/behat-asciidoc-formatter",
    "type": "behat-extension",
    "description": "Behat AsciiDoc Formatter",
    "keywords": ["behat", "asciidoc", "documentation"],
    "autoload": {
        "psr-4": {
            "Digitalnoise\\Behat\\AsciiDocFormatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitalnoise\\Behat\\AsciiDocFormatter\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mikey179/vfsstream": "^1.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Weinke",
            "email": "philip.weinke@digitalnoise.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "behat/behat": "^3.5",
        "symfony/filesystem": "^3.0|^4.0|^5.0"
    }
}
